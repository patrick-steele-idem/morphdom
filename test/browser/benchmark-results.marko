<macro times(moduleName, testName)>
    ${data.getAverageTimeForTest(moduleName, testName)}
</macro>

<div class="results">
    <ul>
        <li for|moduleName| of=data.moduleNames>
            Total time for ${moduleName}: ${data.getTotalTime(moduleName)}
            <if(data.isWinner(moduleName))>
                (winner)
            </if>
        </li>
    </ul>
    <table>
        <thead>
            <tr>
                <td></td>
                <td for|moduleName| of=data.moduleNames>
                    ${moduleName}
                </td>
            </tr>
        </thead>
        <tbody>
            <tr for|testName| for=data.testNames>
                <td class="test-name">
                    ${testName}
                </td>
                <td for|moduleName| of=data.moduleNames>
                    <if(data.isWinnerForTest(moduleName, testName))>
                        <b><times(moduleName, testName)/></b>
                    </if>
                    <else>
                        <times(moduleName, testName)/>
                    </else>
                </td>
            </tr>
        </tbody>
    </table>
</div>
